    <!--  Add Resources Here  -->
    <converters:TextFormattingConverter x:Key="TextFormattingConverter" />
    <converters:ListDateTimesTextConverter x:Key="ConverterListDateTimesText" />
    <converters:WeekDayConverter x:Key="WeekDayConverter" />
    <converters:MonthConverter x:Key="MonthConverter" />
    <ControlTheme TargetType="u:CronPicker" x:Key="{x:Type u:CronPicker}">
        <Setter Property="CopySuccessMessage" Value="{DynamicResource CronPickerFormatter_CopySuccess}" />
        <Setter Property="FailedToGetClipboardMessage" Value="{DynamicResource CronPickerFormatter_FailedToGetClipboard}" />
        <Setter Property="CronExpressionParsedMessage" Value="{DynamicResource CronPickerFormatter_CronExpressionParsed}" />
        <Setter Property="CronExpressionParamsNotEnoughErrorMessage" Value="{DynamicResource CronPickerFormatter_CronExpressionParamsNotEnoughError}" />
        <Setter Property="SecondsExpressionParseErrorFormat" Value="{DynamicResource CronPickerFormatter_SecondsExpressionParseError}" />
        <Setter Property="MinutesExpressionParseErrorFormat" Value="{DynamicResource CronPickerFormatter_MinutesExpressionParseError}" />
        <Setter Property="HoursExpressionParseErrorFormat" Value="{DynamicResource CronPickerFormatter_HoursExpressionParseError}" />
        <Setter Property="DaysOfMonthExpressionParseErrorFormat" Value="{DynamicResource CronPickerFormatter_DaysOfMonthExpressionParseError}" />
        <Setter Property="MonthsExpressionParseErrorFormat" Value="{DynamicResource CronPickerFormatter_MonthsExpressionParseError}" />
        <Setter Property="DaysOfWeekExpressionParseErrorFormat" Value="{DynamicResource CronPickerFormatter_DaysOfWeekExpressionParseError}" />
        <Setter Property="YearsExpressionParseErrorFormat" Value="{DynamicResource CronPickerFormatter_YearsExpressionParseError}" />
        <Setter Property="RecentRunTimesTitleFormat" Value="{DynamicResource CronPickerFormatter_RecentRunTimesTitle}" />
        <Setter Property="Template">
            <ControlTemplate TargetType="u:CronPicker">
                <Border
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalAlignment}">
                    <StackPanel Orientation="Vertical" Spacing="8">
                        <TabControl Name="PART_TabControl" Padding="10,18,10,5">
                            <TabControl.Styles>
                                <Style Selector="u|CronPickerFieldTabItem">
                                    <Setter Property="Padding" Value="15,5,15,4" />
                                    <Setter Property="FontSize" Value="16" />
                                    <Setter Property="VerticalContentAlignment" Value="Center" />
                                </Style>
                            </TabControl.Styles>
                        </TabControl>
                        <u:Divider Content="{DynamicResource CronPicker_PartTitle_CronFieldValues}" FontSize="18" />
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <StackPanel.Styles>
                                <Style Selector="TextBox">
                                    <Setter Property="MaxWidth" Value="120" />
                                    <Setter Property="MinWidth" Value="80" />
                                </Style>
                            </StackPanel.Styles>
                            <TextBox
                                InnerRightContent="{DynamicResource CronFieldHeader_Second}"
                                IsReadOnly="True"
                                Text="{TemplateBinding Seconds}" />
                            <TextBox
                                InnerRightContent="{DynamicResource CronFieldHeader_Minute}"
                                IsReadOnly="True"
                                Text="{TemplateBinding Minutes}" />
                            <TextBox
                                InnerRightContent="{DynamicResource CronFieldHeader_Hour}"
                                IsReadOnly="True"
                                Text="{TemplateBinding Hours}" />
                            <TextBox
                                InnerRightContent="{DynamicResource CronFieldHeader_DayOfMonth}"
                                IsReadOnly="True"
                                Text="{TemplateBinding DaysOfMonth}" />
                            <TextBox
                                InnerRightContent="{DynamicResource CronFieldHeader_Month}"
                                IsReadOnly="True"
                                Text="{TemplateBinding Months}" />
                            <TextBox
                                InnerRightContent="{DynamicResource CronFieldHeader_DayOfWeek}"
                                IsReadOnly="True"
                                Text="{TemplateBinding DaysOfWeek}" />
                            <TextBox
                                InnerRightContent="{DynamicResource CronFieldHeader_Year}"
                                IsReadOnly="True"
                                Text="{TemplateBinding Years}" />
                        </StackPanel>
                        <u:Divider Content="{DynamicResource CronPicker_PartTitle_CronExpression}" FontSize="18" />
                        <TextBox Name="PART_CronExpression" Text="{TemplateBinding CronExpression, Mode=TwoWay}" />
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <Button
                                Content="{DynamicResource CronPicker_ButtonText_CronExpressionParse}"
                                Name="PART_ParseButton"
                                Theme="{DynamicResource SolidButton}" />
                            <Button
                                Content="{DynamicResource CronPicker_ButtonText_CronExpressionCopy}"
                                Name="PART_CopyButton"
                                Theme="{DynamicResource SolidButton}" />
                            <u:Banner
                                CanClose="True"
                                Content="{TemplateBinding CurrentOperationResultMessage}"
                                HorizontalContentAlignment="Left"
                                IsVisible="{TemplateBinding IsCurrentOperationResultVisible,
                                                            Mode=TwoWay}"
                                Type="{TemplateBinding CurrentOperationResultType}">
                                <u:Banner.Template>
                                    <ControlTemplate TargetType="u:Banner">
                                        <Border
                                            BorderBrush="{TemplateBinding BorderBrush}"
                                            BorderThickness="{TemplateBinding BorderThickness}"
                                            Name="PART_Container"
                                            Padding="{TemplateBinding Padding}">
                                            <DockPanel>
                                                <!--  Icon Area  -->
                                                <Panel
                                                    IsVisible="{TemplateBinding ShowIcon}"
                                                    Margin="{DynamicResource ToastCardIconMargin}"
                                                    VerticalAlignment="Top">
                                                    <ContentPresenter Content="{TemplateBinding Icon}" />
                                                    <PathIcon
                                                        Classes="Large"
                                                        IsVisible="{TemplateBinding Icon,
                                                                                    Converter={x:Static ObjectConverters.IsNull}}"
                                                        Name="PART_BuildInIcon"
                                                        Theme="{DynamicResource InnerPathIcon}" />
                                                </Panel>
                                                <ContentControl
                                                    Content="{TemplateBinding Content}"
                                                    Margin="{DynamicResource ToastCardContentMargin}"
                                                    MaxWidth="{DynamicResource ToastCardContentMaxWidth}"
                                                    VerticalContentAlignment="Center"
                                                    x:Name="PART_Content">
                                                    <ContentControl.DataTemplates>
                                                        <DataTemplate DataType="u:IToast">
                                                            <SelectableTextBlock
                                                                FontWeight="{DynamicResource ToastCardContentFontWeight}"
                                                                Foreground="{DynamicResource ToastCardContentForeground}"
                                                                Text="{Binding Content}"
                                                                TextWrapping="Wrap" />
                                                        </DataTemplate>
                                                        <DataTemplate DataType="x:String">
                                                            <SelectableTextBlock
                                                                FontWeight="{DynamicResource ToastCardContentFontWeight}"
                                                                Foreground="{DynamicResource ToastCardContentForeground}"
                                                                Text="{Binding}"
                                                                TextWrapping="Wrap" />
                                                        </DataTemplate>
                                                    </ContentControl.DataTemplates>
                                                </ContentControl>
                                                <!--<ContentPresenter
                                                    Content="{TemplateBinding Content}"
                                                    ContentTemplate="{TemplateBinding ContentTemplate}"
                                                    TextWrapping="Wrap" />-->
                                                <Button
                                                    HorizontalAlignment="Right"
                                                    IsVisible="{TemplateBinding CanClose}"
                                                    Margin="{DynamicResource BannerCloseButtonMargin}"
                                                    Name="{x:Static u:Banner.PART_CloseButton}"
                                                    Theme="{DynamicResource OverlayCloseButton}"
                                                    VerticalAlignment="Top" />
                                            </DockPanel>
                                        </Border>
                                    </ControlTemplate>
                                </u:Banner.Template>
                            </u:Banner>
                            <!--<u:ToastCard
                                    Classes="Light"
                                    Content="{TemplateBinding CurrentOperationResultMessage}"
                                    IsVisible="{TemplateBinding IsCurrentOperationResultVisible,
                                                                Mode=TwoWay}"
                                    MaxHeight="200"
                                    MaxWidth="400"
                                    Name="PART_ToastCard"
                                    NotificationType="{TemplateBinding CurrentOperationResultType}"
                                    ShowClose="False" />
                                <Button
                                    CornerRadius="50"
                                    HorizontalAlignment="Right"
                                    IsVisible="{TemplateBinding IsCurrentOperationResultVisible}"
                                    Theme="{StaticResource OverlayCloseButton}"
                                    VerticalAlignment="Top"
                                    u:CronPicker.CloseOnClick="True"
                                    x:Name="PART_CloseButton" />-->
                            <!--<u:NotificationCard
                                Content="{TemplateBinding CurrentOperationResultMessage,
                                                          Mode=TwoWay}"
                                IsClosed="{TemplateBinding IsCurrentOperationResultNotificationCardClosed,
                                                           Mode=TwoWay}"
                                IsVisible="{TemplateBinding IsCurrentOperationResultVisible,
                                                            Mode=TwoWay}"
                                Name="PART_NotificationCard"
                                NotificationType="{TemplateBinding CurrentOperationResultType,
                                                                   Mode=TwoWay}" />-->
                        </StackPanel>
                        <u:Divider Content="{TemplateBinding NextRunTimes, Converter={StaticResource TextFormattingConverter}, ConverterParameter={TemplateBinding RecentRunTimesTitleFormat}}" FontSize="18">
                            <!--<u:Divider.Content>
                                <TextBlock Text="{TemplateBinding NextRunTimes, Converter={StaticResource TextFormattingConverter}, ConverterParameter={TemplateBinding}}" />
                            </u:Divider.Content>-->
                        </u:Divider>
                        <TextBox
                            Classes="TextArea"
                            IsReadOnly="True"
                            Name="PART_NextRunTimes"
                            Text="{TemplateBinding NextRunTimes,
                                                   Mode=OneWay,
                                                   Converter={StaticResource ConverterListDateTimesText},
                                                   ConverterParameter={TemplateBinding DateTimeFormat}}" />
                    </StackPanel>
                </Border>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
    <ControlTheme
        BasedOn="{StaticResource {x:Type u:CronPickerFieldItem}}"
        TargetType="u:SecondsCronPickerFieldItem"
        x:Key="{x:Type u:SecondsCronPickerFieldItem}">
        <Setter Property="Header" Value="{DynamicResource CronFieldHeader_Second}" />
        <Setter Property="IsEverySecondMode" Value="True" />
        <Setter Property="Template">
            <ControlTemplate TargetType="u:SecondsCronPickerFieldItem">
                <Border
                    Background="{TemplateBinding Background}"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    Padding="{TemplateBinding Padding}">
                    <StackPanel Orientation="Vertical">
                        <CheckBox
                            Content="{DynamicResource CronFieldModeName_All_Second}"
                            HorizontalAlignment="Left"
                            IsChecked="{TemplateBinding IsEverySecondMode,
                                                        Mode=TwoWay}"
                            MinWidth="100"
                            Name="PART_All"
                            Theme="{DynamicResource CardCheckBox}" />
                        <StackPanel Orientation="Horizontal" Spacing="20">
                            <CheckBox
                                Content="{DynamicResource CronFieldModeName_Range_Second}"
                                IsChecked="{TemplateBinding IsRangeLimitMode,
                                                            Mode=TwoWay}"
                                MinWidth="100"
                                Name="PART_RangeLimit"
                                Theme="{DynamicResource CardCheckBox}" />
                            <StackPanel IsEnabled="{Binding #PART_RangeLimit.IsChecked}" Orientation="Horizontal">
                                <TextBlock Text="{DynamicResource CronFieldModeElement_Left_Range_Second}" VerticalAlignment="Center" />
                                <u:NumericByteUpDown
                                    Maximum="58"
                                    Minimum="1"
                                    Value="{TemplateBinding RangeStart,
                                                            Mode=TwoWay}" />
                                <TextBlock Text="{DynamicResource CronFieldModeElement_Middle_Range_Second}" VerticalAlignment="Center" />
                                <u:NumericByteUpDown
                                    Maximum="59"
                                    Minimum="1"
                                    Value="{TemplateBinding RangeEnd,
                                                            Mode=TwoWay}" />
                                <TextBlock Text="{DynamicResource CronFieldModeElement_Right_Range_Second}" VerticalAlignment="Center" />
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="20">
                            <CheckBox
                                Content="{DynamicResource CronFieldModeName_Interval_Second}"
                                IsChecked="{TemplateBinding IsIntervalMode,
                                                            Mode=TwoWay}"
                                MinWidth="100"
                                Name="PART_Interval"
                                Theme="{DynamicResource CardCheckBox}" />
                            <StackPanel IsEnabled="{Binding #PART_Interval.IsChecked}" Orientation="Horizontal">
                                <TextBlock Text="{DynamicResource CronFieldModeElement_Left_Interval_Second}" VerticalAlignment="Center" />
                                <u:NumericByteUpDown
                                    Maximum="59"
                                    Minimum="1"
                                    Value="{TemplateBinding IntervalStartSecond,
                                                            Mode=TwoWay}" />
                                <TextBlock Text="{DynamicResource CronFieldModeElement_Middle_Interval_Second}" VerticalAlignment="Center" />
                                <u:NumericByteUpDown
                                    Maximum="59"
                                    Minimum="1"
                                    Value="{TemplateBinding IntervalValue,
                                                            Mode=TwoWay}" />
                                <TextBlock Text="{DynamicResource CronFieldModeElement_Right_Interval_Second}" VerticalAlignment="Center" />
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Vertical" Spacing="10">
                            <CheckBox
                                Content="{DynamicResource CronFieldModeName_List_Second}"
                                HorizontalAlignment="Left"
                                IsChecked="{TemplateBinding IsListMode,
                                                            Mode=TwoWay}"
                                MinWidth="100"
                                Name="PART_List"
                                Theme="{DynamicResource CardCheckBox}" />
                            <!--<u:MultiComboBox
                            IsEnabled="{Binding #PART_List.IsChecked}"
                            ItemsSource="{TemplateBinding AllowedSeconds}"
                            MaxWidth="400"
                            MinWidth="100">
                            <u:MultiComboBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <WrapPanel MaxWidth="400" MinWidth="100" />
                                </ItemsPanelTemplate>
                            </u:MultiComboBox.ItemsPanel>
                        </u:MultiComboBox>-->
                            <ListBox
                                HorizontalAlignment="Left"
                                IsEnabled="{Binding #PART_List.IsChecked}"
                                ItemsSource="{TemplateBinding AllowedSeconds}"
                                MaxWidth="780"
                                MinWidth="100"
                                SelectedItems="{TemplateBinding SelectedSeconds}"
                                SelectionMode="Multiple"
                                Theme="{DynamicResource MultiplePureCardCheckGroupListBox}">
                                <ListBox.Styles>
                                    <Style Selector="ListBoxItem">
                                        <Setter Property="Width" Value="50" />
                                        <Setter Property="Height" Value="50" />
                                        <Setter Property="Padding" Value="0" />
                                    </Style>
                                </ListBox.Styles>
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <WrapPanel Orientation="Horizontal" />
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                            </ListBox>
                        </StackPanel>
                    </StackPanel>
                </Border>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
