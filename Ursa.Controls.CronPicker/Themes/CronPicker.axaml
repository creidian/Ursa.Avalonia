<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:converters="clr-namespace:Ursa.Themes.Semi.Converters;assembly=Ursa.Themes.Semi"
    xmlns:u="https://irihi.tech/ursa"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Design.PreviewWith>
        <TabControl Height="400" Width="800">
            <TabItem Header="ALL">
                <ScrollViewer>
                    <u:CronPicker />
                </ScrollViewer>
            </TabItem>
            <TabItem Header="秒">
                <ScrollViewer>
                    <StackPanel>
                        <TextBox Text="{Binding #SecondsField.Value}" Width="100" />
                        <u:SecondsCronPickerFieldItem Name="SecondsField" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="分">
                <ScrollViewer>
                    <StackPanel>
                        <TextBox Text="{Binding #MinutesField.Value}" Width="100" />
                        <u:MinutesCronPickerFieldItem Name="MinutesField" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="时">
                <ScrollViewer>
                    <StackPanel>
                        <TextBox Text="{Binding #HoursField.Value}" Width="100" />
                        <u:HoursCronPickerFieldItem Name="HoursField" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="日">
                <ScrollViewer>
                    <StackPanel>
                        <TextBox Text="{Binding #DaysOfMonthField.Value}" Width="100" />
                        <u:DaysOfMonthCronPickerFieldItem Name="DaysOfMonthField" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="月">
                <ScrollViewer>
                    <StackPanel>
                        <TextBox Text="{Binding #MonthsField.Value}" Width="100" />
                        <u:MonthsCronPickerFieldItem Name="MonthsField" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="周">
                <ScrollViewer>
                    <StackPanel>
                        <TextBox Text="{Binding #DaysOfWeekField.Value}" Width="100" />
                        <u:DaysOfWeekCronPickerFieldItem Name="DaysOfWeekField" />
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
            <TabItem Header="年">
                <StackPanel>
                    <TextBox Text="{Binding #YearsField.Value}" Width="100" />
                    <u:YearsCronPickerFieldItem Name="YearsField" />
                </StackPanel>
            </TabItem>
            <TabItem Header="测试">
